- if (typeof(width) === 'undefined') width = 240;
- if (typeof(height) === 'undefined') height = 200;
- if (typeof(name) !== 'undefined')
  h1= name

.widget
  - if (typeof(links) !== 'undefined' && links)
    nav.linkup
      ul
        li
          a.icn.cut.embed(href=url_with('/embed', {widget: 'donors'}))
            span.onshow embed
        li.onshow
          a.icn.favourite(href=url, data-dashkey="widgets")
            | save to dashboard
  iframe(src=url, scrolling='no', style='width: #{width}px; height: #{height}px; border: 0px;')